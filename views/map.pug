html
    head
        title Mapped Data
        link(rel="stylesheet" href="https://unpkg.com/leaflet@1.6.0/dist/leaflet.css")
        link(rel="stylesheet" href="/stylesheets/mapStyles.css")
    body
        div(id="map")
        select(name="typeSelect" id="typeSelect")
            option(value="invalid") Please select a data type to plot
            - for (var counter = 0; counter < types.length; counter++)
                option(value=types[counter].type) #{types[counter].type}

        script(src="https://unpkg.com/leaflet@1.6.0/dist/leaflet.js" integrity="sha512-gZwIG9x3wUXg2hdXF6+rVkLF/0Vi9U8D2Ntg4Ga5I5BZpVkVxlJWbSQtXPSiUTtC0TjtGOmxa1AJPuV0CPthew==" crossorigin="")
        script(src="/javascripts/vendor/leaflet-providers.js")
        script(src="/javascripts/mapScript.js")